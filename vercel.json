{
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "www.baustellen-timelapse-leipzig.de"
        }
      ],
      "destination": "https://baustellen-timelapse-leipzig.de/",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.baustellen-timelapse-leipzig.de"
        }
      ],
      "destination": "https://baustellen-timelapse-leipzig.de/:path*",
      "permanent": true
    }
  ]
}
